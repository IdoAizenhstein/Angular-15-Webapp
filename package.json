{
  "name": "bizibox",
  "version": "15.0.2",
  "author": "Ido Aizenshtein, Vladislav Bogdan",
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=25000 ./node_modules/@angular/cli/bin/ng serve --hmr=true --open=true",
    "build": "ng build --aot=true --build-optimizer=true --optimization=true --named-chunks=true",
    "build-compress": "ng build && gzipper c ../../../target/frontend --include js,css,html --output-file-format [filename].[ext] ../../../target/frontend-compressed",
    "watch": "ng build --watch --configuration development",
    "bundle-analyzer": "ng build --stats-json",
    "analyzer": "webpack-bundle-analyzer ../../../target/frontend/stats.json",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.2",
    "@angular/cdk": "^15.0.2",
    "@angular/common": "^15.0.2",
    "@angular/compiler": "^15.0.2",
    "@angular/core": "^15.0.2",
    "@angular/forms": "^15.0.2",
    "@angular/material": "^15.0.2",
    "@angular/material-moment-adapter": "^15.0.2",
    "@angular/platform-browser": "^15.0.2",
    "@angular/platform-browser-dynamic": "^15.0.2",
    "@angular/router": "^15.0.2",
    "@ng-bootstrap/ng-bootstrap": "^14.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.10.2",
    "@turf/area": "^6.5.0",
    "@turf/boolean-contains": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@vimeo/player": "^2.18.0",
    "angular-highcharts": "^14.1.7",
    "angulartics2": "^12.2.0",
    "highcharts": "^10.3.2",
    "jquery": "^3.6.1",
    "jquery-ui": "^1.13.2",
    "jspdf": "^2.5.1",
    "jwt-decode": "^3.1.2",
    "moment-timezone": "^0.5.40",
    "network-information-types": "^0.1.1",
    "ngx-scrollbar": "^10.0.1",
    "primeicons": "^6.0.1",
    "primeng": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.2",
    "@angular/localize": "^15.0.2",
    "@types/vimeo__player": "^2.16.3",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1"
  }
}
